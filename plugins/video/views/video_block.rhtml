<h3 class="block-title">
  <span><%=block.title%></span>
</h3>
<div class="video-block-data">
  <% if block.is_youtube? %>
    <%= render :partial => 'box_organizer/iframe_video_block.rhtml', :locals => { :url => block.format_embed_video_url_for_youtube, :width => block.width, :height => block.height }%>
  <% elsif block.is_vimeo? %>
    <%= render :partial => 'box_organizer/iframe_video_block.rhtml', :locals => { :url => block.format_embed_video_url_for_vimeo, :width => block.width, :height => block.height }%>
  <% elsif block.is_video_file? %>
    <%= render :partial => 'box_organizer/html5_video_block.rhtml', :locals => { :url => block.url, :width => block.width, :height => block.height }%>
  <% else %>
    <%= "<span class='alert-block'>"+_("Cadastre uma nova URL (Vimeo, Youtube)")+"</span>" %>
  <% end %>

</div>
